chunkl.all: $(chunkls)
	cat $(chunkls) > chunkl.all

%.c.cc: %.c | chunkl.all
	perl $(top_srcdir)/chunkl.pl $*.c < $< > $@
%.h.hh: %.h | chunkl.all
	perl $(top_srcdir)/chunkl.pl $*.h < $< > $@
%.h.chunkl: %.h
	perl $(top_srcdir)/chunkl.pl $*.h --header < $< > $@
%.c.chunkl: %.c
	perl $(top_srcdir)/chunkl.pl $*.c --header < $< > $@
%.cc.o: %.cc
	$(AM_V_CC)$(CC) -c $(CPPFLAGS) $(ALL_CFLAGS) $(PROFILING_CFLAGS) $< -o $@

